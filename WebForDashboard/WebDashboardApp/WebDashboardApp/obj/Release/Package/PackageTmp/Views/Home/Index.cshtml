<img src='@Url.Action("GetActivityCountImage","Home")' width="300" height="300" />
<img src='@Url.Action("GetMoodCountImage","Home")' width="260" height="260" />
<img src='@Url.Action("GetMoodWeeklyImage","Home")' width="260" height="260" />
<p id="info"></p>
<div class="col-sm-12" align="right" style="max-width:300px;">
    <button class="btn btn-primary" onclick="btnClick()">Back</button>
</div>
<br /><br />
<label>User Agent:</label>@ViewBag.UserAgentStr<br /><br />
<script>
    document.addEventListener("message", function () {
        document.getElementById("info").innerHTML += message.data;
    });
    function btnClick() { window.postMessage("Hello React Native!"); }
</script>